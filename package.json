{
  "productName": "hands",
  "name": "hands",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "start": "APP_DEV=true electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "@mediapipe/camera_utils": "^0.3.1640029074",
    "@mediapipe/hands": "^0.4.1635986972",
    "electron-squirrel-startup": "^1.0.0",
